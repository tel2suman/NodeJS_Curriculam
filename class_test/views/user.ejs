    <!-- Card -->

    <div class="container">
        <div class="row row-cols-1 row-cols-sm-2 row-cols-md-3 g-3 my-5 py-3">
            <% data.map(user =>{ %>
                <div class="col">
                    <div class="card shadow-sm">
                        <!-- <img src="<%= user.thumbnailUrl %>" class="card-img-top" alt="..."> -->
                        <div class="card-body">
                            <h3 class="card-title lead fw-bold">
                                <%= user.name %>
                            </h3>
                            <h5 class="card-subtitle lead mb-2 fw-normal">
                                <%= user.username %>
                            </h5>
                            <h4 class="card-subtitle lead mb-2 fw-normal">
                                <%= user.email %>
                            </h4>
                            <h4 class="card-subtitle lead mb-2 fw-normal">
                                <%= user.phone %>
                            </h4>
                            <h4 class="card-subtitle lead mb-2 fw-normal">
                                <%= user.website %>
                            </h4>
                            <p class="card-text lead fw-normal">
                                <%= user.title %>
                            </p>
                        </div>
                    </div>
                </div>
            <% }); %>
        </div>
    </div>

